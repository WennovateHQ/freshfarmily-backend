{
  "name": "freshfarmily-backend",
  "version": "1.0.0",
  "description": "FreshFarmily API Service for Farm-to-Table Delivery",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "mocha src/tests/**/*.js --timeout 10000",
    "test:integration": "mocha src/tests/integration/*.js --timeout 10000",
    "test:unit": "mocha src/tests/unit/*.js",
    "setup": "node setup-production.js",
    "lint": "eslint src/",
    "seed": "node src/scripts/seedDatabase.js",
    "seed:all": "node scripts/seed-all-data.js",
    "seed:all:reset": "cross-env SEED_RESET_DB=true node scripts/seed-all-data.js",
    "seed:farms": "node scripts/seed-farms.js",
    "seed:simple": "node scripts/simple-seed.js",
    "seed:production": "node scripts/production-seed.js",
    "seed:production:reset": "cross-env SEED_RESET_DB=true node scripts/production-seed.js",
    "seed:production:log": "node scripts/production-seed-with-logging.js",
    "seed:production:log:reset": "cross-env SEED_RESET_DB=true node scripts/production-seed-with-logging.js"
  },
  "keywords": [
    "freshfarmily",
    "express",
    "postgresql",
    "api"
  ],
  "author": "FreshFarmily Team",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.41",
    "axios": "^1.8.2",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.3.0",
    "geolib": "^3.3.4",
    "helmet": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.14.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.30.0",
    "sequelize-cli": "^6.6.2",
    "sqlite3": "^5.1.6",
    "stripe": "^17.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "uuid": "^9.0.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "chai": "^5.2.0",
    "chai-http": "^5.1.1",
    "cross-env": "^7.0.3",
    "jest": "^29.5.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.9",
    "sinon": "^19.0.2",
    "supertest": "^6.3.3",
    "umzug": "^3.8.2"
  }
}
